{
    "name": "mern",
    "version": "0.1.0",
    "private": true,
    "proxy": "http://localhost:3001/",
    "dependencies": {
      "react": "^16.6.3",
      "react-dom": "^16.6.3",
      "react-scripts": "^2.1.1"
    },
    "scripts": {
      "start": "react-scripts start",
      "build": "react-scripts build",
      "test": "react-scripts test --env=jsdom",
      "eject": "react-scripts eject"
    },
    "browserslist": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ]
  }
  {
    "name": "mern",
    "version": "1.0.0",
    "description": "Mern Demo",
    "main": "server.js",
    "scripts": {
      "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
      "start:prod": "node server",
      "start:dev": "concurrently \"nodemon --ignore 'client/*' server \" \"npm run client\"",
      "client": "cd client && npm run start",
      "seed": "node scripts/seedDB.js",
      "install": "npm run install-client && npm run install-server",
      "i-c": "npm run install-client",
      "i-s": "npm run install-server",
      "un-c": "npm run uninstall-client",
      "un-s": "npm run uninstall-server",
      "install-client": "cd client && npm install",
      "install-server": "cd server && npm install",
      "uninstall-client": "cd client && npm uninstall",
      "uninstall-server": "cd server && npm uninstall",
      "build": "cd client && npm run build",
      "heroku-postbuild": "npm run build"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
      "concurrently": "^4.1.0",
      "nodemon": "^1.18.7"
    },
    "dependencies": {
      "if-env": "^1.0.4"
    }
  }
  